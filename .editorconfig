; This file is for unifying the coding style for different editors and IDEs.
; More information at http://EditorConfig.org

root = true

[*]
trim_trailing_whitespace = true
insert_final_newline = true
indent_style = space
indent_size = 4

[*.ps1]
charset = utf-8
end_of_line = crlf

[*.sh]
end_of_line = LF

[*.DotSettings]
end_of_line = LF
